uest({\r\n    url: '/sign/editPassword',\r\n    method: 'post',\r\n    data: { userId, password }\r\n  })\r\n}\r\n// 修改密码\r\nfunction getDepartment(parentId) {\r\n  return request({\r\n    url: '/sign/getDepartment',\r\n    method: 'post',\r\n    data: { parentId }\r\n  })\r\n}\r\n"]}]}