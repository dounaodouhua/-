;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,KAHA;AAIA,IAAA,UAJA,sBAIA,GAJA,EAIA;AACA,WAAA,WAAA,IAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,KAPA;AAQA,IAAA,OARA,mBAQA,IARA,EAQA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,aAAA,CAAA;AACA,kBAAA,aAAA,EAAA,IADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA,WAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,GAHA;AAQA,gBAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,MAAA;AACA,gBAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,aAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,UAAA;;AAZA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KAxBA;AAyBA,IAAA,WAzBA,uBAyBA,KAzBA,EAyBA;AACA,UAAA,KAAA,GAAA,CAAA,GAAA,EAAA,EAAA;AACA,0BAAA,KAAA,GAAA,CAAA;AACA,OAFA,MAEA;AACA,yBAAA,KAAA,GAAA,CAAA;AACA;AACA;AA/BA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div v-loading=\"loading\" class=\"WorkGuide\">\r\n    <div class=\"header\">\r\n      <span class=\"title\">{{ detail.channelCName }}</span>\r\n      <div class=\"btn-group\">\r\n        <el-button type=\"primary\" :title=\"currentPage===1?'当前已是第一篇':''\" :disabled=\"currentPage===1\" icon=\"el-icon-arrow-left\" size=\"mini\" @click=\"changePage(-1)\">上一篇</el-button>\r\n        <el-button type=\"primary\" :title=\"totalElements===currentPage?'当前已是最后一篇':''\" :disabled=\"totalElements===currentPage\" size=\"mini\" @click=\"changePage(1)\">下一篇<i class=\"el-icon-arrow-right el-icon--right\" /></el-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"time\">\r\n      <svg-icon icon-class=\"axt-time\" style=\"margin-right:7px\" />\r\n      <span>{{ detail.channelCTime | filterDateTime }}</span>\r\n      <span style=\"margin: 0 13px\">|</span>\r\n      <svg-icon icon-class=\"eye\" style=\"margin-right:6px\" />\r\n      <span>{{ detail.hitTimes }}</span>\r\n    </div>\r\n    <div class=\"text v-html\" v-html=\"detail.channelCText\" />\r\n  </div>\r\n</template>\r\n<script>\r\nimport moment from 'moment'\r\nimport webAPI from '@/api/website'\r\nimport adaptive from '@/directive/el-table'\r\nexport default {\r\n  name: 'WorkGuide',\r\n  directives: { adaptive },\r\n  filters: {\r\n    filterDateTime(val) {\r\n      return moment(val).format('YYYY/MM/DD')\r\n    }\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      detail: {},\r\n      currentPage: 1,\r\n      totalElements: 0\r\n    }\r\n  },\r\n  created() {\r\n    this.getData(this.$route.meta.id)\r\n  },\r\n  methods: {\r\n    async addReadNum() {\r\n      await webAPI.readNum(this.detail.id)\r\n    },\r\n    changePage(num) {\r\n      this.currentPage += num\r\n      this.getData(this.$route.meta.id)\r\n    },\r\n    async getData(type) {\r\n      try {\r\n        this.loading = true\r\n        const res = await webAPI.columnContent({\r\n          channelTypeId: type,\r\n          page: this.currentPage,\r\n          size: 1\r\n        })\r\n        this.detail = res.content[0]\r\n        console.log(this.detail)\r\n        this.totalElements = res.totalElements\r\n        this.loading = false\r\n        this.addReadNum()\r\n      } catch (error) {\r\n        this.loading = false\r\n      }\r\n    },\r\n    indexMethod(index) {\r\n      if (index + 1 < 10) {\r\n        return `0${index + 1}`\r\n      } else {\r\n        return `${index + 1}`\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .text  table {\r\n  border-collapse: collapse;\r\n  border-spacing: 0;\r\n  width: calc(100% - 20px) !important;\r\n  p{\r\n    margin: 0;\r\n  }\r\n    tbody{\r\n      tr:nth-child(1){\r\n        background-color:#F8FAFF;\r\n        td{\r\n          font-weight: bold;\r\n          span{\r\n            font-family: PingFang SC !important;\r\n          }\r\n        }\r\n      }\r\n    }\r\n}\r\n::v-deep .text td {\r\n  border:1px solid #F2F2F2 !important;\r\n  height: 44px;\r\n  line-height: 44px;\r\n  border-collapse: collapse;\r\n    border-spacing: 0;\r\n}\r\n::v-deep .text td {\r\n  border:1px solid #000;\r\n}\r\n::v-deep.WorkGuide {\r\n  font-family: PingFang SC;\r\n  margin-bottom: 10px;\r\n  .header {\r\n    margin-top: 16px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    padding-right: 20px;\r\n    .title {\r\n      width: 700px;\r\n      font-size: 24px;\r\n      font-weight: bold;\r\n      color: #000000;\r\n      letter-spacing: 2px;\r\n    }\r\n    .btn-group {\r\n      .el-button {\r\n        color: #3776ff;\r\n        background-color: #fff;\r\n        border: 1px solid #3776ff;\r\n        font-size: 14px;\r\n        padding: 7px 10px;\r\n        transition: all 0.3s;\r\n        &:hover {\r\n          background-color: #ecf5ff;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .time {\r\n    margin-top: 12px;\r\n    color: #999999;\r\n    font-size: 12px;\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/website-page/pages"}]}