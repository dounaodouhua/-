A,IAAA,OADA,mBACA,IADA,EACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,aAAA,CAAA;AACA,kBAAA,aAAA,EAAA,IADA;AAEA,kBAAA,IAAA,EAAA,KAAA,CAAA,WAFA;AAGA,kBAAA,IAAA,EAAA,KAAA,CAAA;AAHA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,GAHA;AAQA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,aAAA;AACA,gBAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,OAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAfA;AAgBA,IAAA,gBAhBA,4BAgBA,GAhBA,EAgBA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,KAnBA;AAoBA,IAAA,mBApBA,+BAoBA,GApBA,EAoBA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA;AAvBA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <transition name=\"fade-transform\" mode=\"out-in\">\r\n    <div v-if=\"!$route.path.includes('Detail')\" v-loading=\"loading\" class=\"Notice\">\r\n      <el-scrollbar :style=\"`height:calc(100vh - 305px)`\">\r\n        <el-empty v-if=\"list.length===0\" description=\"暂无数据\" />\r\n        <TitleAndThumbnail v-for=\"(item, index) in list\" :key=\"item.id\" :detail=\"item\" :has-bottom-border=\"!(index+1===list.length)\" path=\"/website/notice/noticeDetail\" />\r\n      </el-scrollbar>\r\n      <div style=\"width:100%;display:flex;justify-content: center;\">\r\n        <el-pagination :current-page.sync=\"currentPage\" :page-sizes=\"[5,25,50,75,100]\" :page-size=\"pageSize\" layout=\"sizes, prev, pager, next\" :total=\"total\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" />\r\n      </div>\r\n    </div>\r\n    <router-view v-else />\r\n  </transition>\r\n</template>\r\n<script>\r\nimport TitleAndThumbnail from '../components/TitleAndThumbnail.vue'\r\nimport webAPI from '@/api/website'\r\nexport default {\r\n  name: 'Notice',\r\n  components: { TitleAndThumbnail },\r\n  data() {\r\n    return {\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      loading: false,\r\n      list: []\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    $route(newVal, oldVal) {\r\n      if (newVal.name === 'Notice' && oldVal.name === 'noticeDetail') {\r\n        this.getData(this.$route.meta.id)\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    if (!this.$route.path.includes('Detail')) {\r\n      this.getData(this.$route.meta.id)\r\n    }\r\n  },\r\n  methods: {\r\n    async getData(type) {\r\n      try {\r\n        this.loading = true\r\n        const res = await webAPI.columnContent({\r\n          channelTypeId: type,\r\n          page: this.currentPage,\r\n          size: this.pageSize\r\n        })\r\n        this.total = res.totalElements\r\n        this.list = res.content\r\n        this.loading = false\r\n      } catch (error) {\r\n        this.loading = false\r\n      }\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      this.getData(this.$route.meta.id)\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.getData(this.$route.meta.id)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"~@/assets/css/variables.scss\";\r\n</style>\r\n"],"sourceRoot":"src/views/website-page/pages"}]}